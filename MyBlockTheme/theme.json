  {
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings":{
      "appearanceTools": true,
      "color": {
        "defaultPalette": false,
        "palette": [
          {
            "slug": "Neutral",
            "color": "#989898",
            "name": "Neutral"
          },
          {
            "slug": "Primary",
            "color": "#0073aa",
            "name": "Primary" 
          },{
            "slug": "Secondary",
            "color": "#d54e21",
            "name": "Secondary" },
          {
            "slug": "Tertiary",
            "color": "#46b450",
            "name": "Tertiary"
          }
        ],
        "gradients": [
          {
            "slug": "primary-gradient",
            "gradient": "linear-gradient(#11364a,#0b7da5)",
            "name": "Primary Gradient Blue"
          }
        ],
        "custom": true,
        "link": true,
        "background": true
      },
      "typography": {
        "fontFamilies": [
          {
            "fontFamily": "'Quantrocento',serif",
            "name": "Quantrocento",
            "slug": "quantrocento",
            "fontFace": [
              {
                "fontFamily": "'Quantrocento',serif",
                "fontStyle": "normal",
                "fontWeight": "400",
                "src": "url('https://fonts.googleapis.com/css2?family=Quattrocento&display=swap')"
              },
              {
                "fontFamily": "'Quantrocento',serif",
                "fontStyle": "italic",
                "fontWeight": "400",
                "src": "url('https://fonts.googleapis.com/css2?family=Quattrocento:ital,wght@1,400&display=swap')"
              },
              {
                "fontFamily": "'Quantrocento',serif",
                "fontStyle": "normal",
                "fontWeight": "700",
                "src": "url('https://fonts.googleapis.com/css2?family=Quattrocento:wght@700&display=swap')"
              },
              {
                "fontFamily": "'Quantrocento',serif",
                "fontStyle": "italic",
                "fontWeight": "700",
                "src": "url('https://fonts.googleapis.com/css2?family=Quattrocento:ital,wght@1,700&display=swap')"
              } 

            
            ]
          }
        ]
      }
    },
    
    "templateParts": {
      "header": {
        "title": "My Header",
        "area": "header"
      }
    }
  }
